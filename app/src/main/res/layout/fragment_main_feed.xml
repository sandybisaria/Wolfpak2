<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/main_feed_base_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="0dp"
    android:padding="0dp">

<ImageView
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/main_feed_no_posts_image_view"
    android:layout_margin="0dp"
    android:padding="0dp"
    android:scaleType="fitXY"
    android:contentDescription="@string/howl"
    android:src="@drawable/wolfpaktest"/>

<ImageButton
    android:layout_width="39dp"
    android:layout_height="39dp"
    android:id="@+id/main_feed_report_button"
    android:contentDescription="@string/report_howl"
    android:src="@drawable/alert"
    android:layout_gravity="end|top"
    android:layout_alignParentTop="true"
    android:layout_alignParentEnd="true" />

